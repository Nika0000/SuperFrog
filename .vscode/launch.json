{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "SuperFrog (debug mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "toolArgs": [
                "--dart-define-from-file",
                "env.dev.json"
            ]
        },
        {
            "name": "SuperFrog (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "--dart-define-from-file",
                "env.prod.json"
            ]
        },
        {
            "name": "SuperFrog Web (Debug mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "toolArgs": [
                "-d",
                "web-server",
                "--web-hostname",
                "localhost",
                "--web-port",
                "8080",
                "--web-renderer",
                "html",
                "--dart-define-from-file",
                "env.prod.json"
            ]
        },   {
            "name": "SuperFrog Web (Release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "-d",
                "web-server",
                "--web-hostname",
                "192.168.202.214",
                "--web-port",
                "8080",
                "--web-renderer",
                "html",
                "--dart-define-from-file",
                "env.prod.json"
            ]
        },
    ]
}